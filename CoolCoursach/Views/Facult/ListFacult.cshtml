@{
}
@model IEnumerable<CoolCoursach.Models.Facult>
<h4 class="display-4">Список специальностей</h4>
        @Html.ActionLink("Добавить специальность", "AddFacult", "Facult")
<table>
    <tr>
        <td>Название специальности</td>
    </tr>
    @foreach(var Facult in Model){
    <tr>
        <td>@Facult.Name</td>
    
    <td>@Html.ActionLink("Редактировать специальность","EditFacult","Facult", new{Id = Facult.Id})</td>
    <td>@Html.ActionLink("Удалить специальность","DeleteFacult","Facult", new{Id = Facult.Id})</td>
        </tr>
    }
</table>